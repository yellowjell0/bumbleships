<section>
<table>
  <% n = 0 %>
  <tr >
    <% 11.times do |i| %>
    <td class="coordinates"><% if i != 0 %><%= i-1 %><% end %></td>
    <% end %>
  </tr>
  <% letters = ("A".."J").to_a %>
  <% letter_counter = 0 %>
  <% spaces.each do |space| %>
  <% if n == 0 %>
  <tr>
    <td class= "coordinates"><%= letters[letter_counter]%></td>
    <% letter_counter += 1 %>
    <% end %>
    <td class="<%=space.coordinate %> <%= space.status %> block"></td>
    <% if n == 9 %>
  </tr>
    <%end%>
    <% if n < 9 %>
    <% n+= 1%>
    <%else %>
    <% n = 0 %>
    <%end %>
    <%end %>
</table>
